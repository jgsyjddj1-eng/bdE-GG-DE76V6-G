local Players = game:GetService("Players")
local player = Players.LocalPlayer

local SelectedTool = "Laptop"
local Shape = "Ø¯Ø§Ø¦Ø±Ø©"
local Radius = 15
local Height = 20
local CollectedTools = {}

TabNM:AddTextBox({
    Name = "Ø§Ù„Ù…Ø³Ø§Ø­Ø©",
    Default = tostring(Radius),
    TextDisappear = false,
    Callback = function(value)
        local n = tonumber(value)
        if n then Radius = math.abs(n) end
    end
})

TabNM:AddTextBox({
    Name = "Ø§Ù„Ø§Ø±ØªÙØ§Ø¹",
    Default = tostring(Height),
    TextDisappear = false,
    Callback = function(value)
        local n = tonumber(value)
        if n then
            Height = n * -1 -- Ø¹ÙƒØ³ Ø§Ù„Ø¥Ø´Ø§Ø±Ø© ØªÙ„Ù‚Ø§Ø¦ÙŠÙ‹Ø§
        end
    end
})

TabNM:AddDropdown({
    Name = "Ø§Ù„Ø§Ø¯Ø§Ø©",
    Options = {"Laptop", "Iphone", "FireX"},
    Default = SelectedTool,
    Callback = function(value)
        SelectedTool = value
    end
})

TabNM:AddDropdown({
    Name = "Ø§Ù„Ø´ÙƒÙ„",
    Options = {"Ø¯Ø§Ø¦Ø±Ø©", "Ù…ÙƒØ¹Ø¨", "ÙƒØ±Ø©"},
    Default = Shape,
    Callback = function(value)
        Shape = value
    end
})

local function organizarItens()
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player:WaitForChild("Backpack")
    local rootPart = character:WaitForChild("HumanoidRootPart")
    local head = character:FindFirstChild("Head") or rootPart

    local tools = {}
    for _, item in pairs(character:GetChildren()) do
        if item:IsA("Tool") and item.Name == SelectedTool then
            table.insert(tools, item)
        end
    end
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") and item.Name == SelectedTool then
            table.insert(tools, item)
        end
    end

    if #tools == 0 then
        warn("Ù„Ø§ ØªÙˆØ¬Ø¯ Ø£Ø¯ÙˆØ§Øª Ø¨Ø§Ø³Ù… '" .. SelectedTool .. "'!")
        return
    end

    CollectedTools = tools

    local center = head.Position + Vector3.new(0, Height, 0)
    local count = #tools

    if Shape == "Ø¯Ø§Ø¦Ø±Ø©" then
        local angleStep = (2 * math.pi) / count
        for i, item in ipairs(tools) do
            local angle = (i - 1) * angleStep
            local x = math.cos(angle) * Radius
            local z = math.sin(angle) * Radius
            local pos = center + Vector3.new(x, 0, z)

            item.Parent = character
            item.GripPos = pos - rootPart.Position
            item.Parent = backpack
        end

    elseif Shape == "Ù…ÙƒØ¹Ø¨" then
        local steps = math.ceil(count^(1/3))
        local spacing = (Radius * 2) / (steps - 1)
        local index = 1

        for ix = 0, steps - 1 do
            for iy = 0, steps - 1 do
                for iz = 0, steps - 1 do
                    if index > count then break end
                    local x = -Radius + ix * spacing
                    local y = -Radius + iy * spacing
                    local z = -Radius + iz * spacing
                    local pos = center + Vector3.new(x, y, z)

                    local item = tools[index]
                    item.Parent = character
                    item.GripPos = pos - rootPart.Position
                    item.Parent = backpack
                    index += 1
                end
            end
        end

    elseif Shape == "ÙƒØ±Ø©" then
        local step = math.pi / 8
        local countIndex = 1
        for a = 0, 2*math.pi, step do
            for b = 0, math.pi, step do
                if countIndex > #tools then break end
                local x = math.sin(a) * math.sin(b) * Radius
                local y = math.cos(b) * Radius
                local z = math.cos(a) * math.sin(b) * Radius
                local pos = center + Vector3.new(x, y, z)
                local item = tools[countIndex]
                item.Parent = character
                item.GripPos = pos - rootPart.Position
                item.Parent = backpack
                countIndex += 1
            end
        end
    end

    print("ØªÙ… ØªÙ†Ø¸ÙŠÙ… "..count.." Ø£Ø¯Ø§Ø© "..SelectedTool.." Ø¨Ø´ÙƒÙ„ "..Shape.." ÙÙˆÙ‚ Ø§Ù„Ø±Ø£Ø³!")
end

TabNM:AddButton({
    Name = "ØªÙØ¹ÙŠÙ„ Ø§Ù„Ø´ÙƒÙ„",
    Callback = function()
        organizarItens()
    end
})
